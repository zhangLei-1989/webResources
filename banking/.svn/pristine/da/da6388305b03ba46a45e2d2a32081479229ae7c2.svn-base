    name: "AlreadyCollect",
    data() {
      return {
        searchMsgbtn: false,
        totalRecord: 0,//数据总条数
        ContractPlanBtn: false, //合同计划 带有时间轴
        currentItem:{}, //当前操作的数据行
        searchMsg: {
          minPrice: '',
          maxPrice: '',
          date: '',
          likeName: ''
