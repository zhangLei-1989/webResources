            realRepayDate:'' ,//
            remark:'',// 备注
            principal: '', //本金
            interest: '',//利息
            lateFee: '',//滞纳金
            confirmId:'', //确认人
            confirmTime:'', // 确认时间
            realPrice:'', //实际还款金额
            planPrice: '', //实收金额
            realRepayDate: '',//时间还款时间
            payeePhone: '' ,// 收款人电话
            payeeName: '' , //收款人
            currDuration:'', //当前分期
