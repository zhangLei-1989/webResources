export default {
    namespaced: true,
    state: {
        corporateDialogActive: false  //是否显示，添加部门弹出框
    },
    mutations: {

        //是否显示，添加部门弹出框
        toogleCorporateDialogActive: function (state, isActive) {
            state.corporateDialogActive = isActive
        }

    }
}