<script>
import contract1 from "@/components/examinaApproval/createContract/Contract1";
export default {
  name: "WaitExaminaApproval",
  data() {
    return {
      loading: true, //整页的loading

      activeDetail: false, //查看详情页的dialog
      activeReject: false, //驳回的dialog
      activeAdopt: false, //通过的dialog
      activeContract1: false, //生成合同1
      activeContract2: false, //生成合同2
      activeContract3: false, //生成合同3
      activeContract4: false, //生成合同4

      formInline: {
        user: "",
        region: ""
      },

      //筛选的过滤信息
      screening: {
        areaValue: "", //城市/区域
        area: [] //城市/区域
      },

      //表格信息
      table: {
        thead: [],
        tbody: [],
        tfoot: [],
        currentPage: 1,
        pageSize: 10,
        totalRecord: 0
      }
    };
  },
  methods: {
    handle() {
      var areaValue = this.screening.areaValue;
      if (areaValue == 1) {
        this.activeDetail = true;
      } else if (areaValue == 2) {
        this.activeContract4 = true;
      }
    },
    detailClose() {
      this.activeDetail = false;
      this.screening.areaValue = "审批";
    },
    rejectClose() {
      this.activeReject = false;
    },
    adoptClose() {
      this.activeAdopt = false;
    },
    contractClose1() {
      this.activeContract1 = false;
      this.screening.areaValue = "审批";
    },
    contractClose2() {
      this.activeContract2 = false;
      this.screening.areaValue = "审批";
    },
    contractClose3() {},
    contractClose4() {}
  },
  created() {
    const self = this;
    //查询列表数据
    this.axios({
      method: "post",
      url: this.$store.state.api.borrow_apply.get_borrowApplyList
    })
      .then(function(response) {
        const { data } = response;
        data.forEach(item => {
          self.table.tbody.push(item);
        });
        self.loading = false;
      })
      .catch(() => {
        self.loading = false;
      });
  }
};
</script>

