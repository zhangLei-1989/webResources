      };

      const contract3_msg = {
        applicantCompany: "公司名称：", //公司名称
        houseAddress: "用款房源：", //用款房源
        houseNumber: "套", //房源套数
        moneyPurpose: "款项用途：", //款项用途
        quota: "放款金额：", //放款金额
        yearRate: "借款利率（年化）：", //借款利率（年化）
        term: "借款期限：" //借款期限
      };

      const contract4_msg = {
        applicantCompany1: "公司名称：", //公司名称 委托人
        applicantCompany2: "公司名称：", //用款房源 受托人
        beginDate: "借款日期：", //借款日期
        price: "借款金额：", //借款金额
        term: "借款期限：", //借款期限
        yearLimit: "年", //借款年限
        moneyPurpose: "借款用途：", //借款用途
        paymentType: "支付方式：" //支付方式
      };

      self.dialog.contract1_msg = contract1_msg;
      self.dialog.contract2_msg = contract2_msg;
      self.dialog.contract3_msg = contract3_msg;
      self.dialog.contract4_msg = contract4_msg;
    },

    /**-----确认生成合同----- */
    sureCreatContract() {
      const self = this;
      const userid = tool.getUserId();
      if (!userid) {
        self.$message.error("登录超时，请先登录！");
        return;
      }
      //如果还在加载中
      if (self.loading.contract1) {
        return;
      }
      if (!self.dialog.contract1_source_data.basePartner_id) {
        self.$message.error("请选择甲方！");
        return;
      }
      if (!self.dialog.contract1_source_data.signTime) {
        self.$message.error("请选择合同签约时间！");
        return;
      }
      self.loading.contract4 = true;
      self
        .axios({
          method: "post",
          url: self.$store.state.api.borrowApply.saveContract,
          data: {
            gcid: "0100099",
            userid: userid,
            params: {
              contractId: self.dialog.contract1_source_data.contractId,
              signDate: self.dialog.contract1_source_data.signTime,
              apartmentId: self.dialog.contract1_source_data.basePartner_id
            }
          }
        })
        .then(response => {
          self.loading.contract4 = false;
          const { data } = response;
          if (data.status.code == 200) {
            self.closeContractAll();
            self.getBorrowApplyList();
            self.$alert("操作成功", "提示");
          }
        })
        .catch(error => {
          self.loading.contract4 = false;
        });
    },

    /**-----点击单行时触发----- */
    tableRowClick(row) {
      const self = this;
      if (row && row.id) {
        const id = row.id;
        self.openDetail(id);
      }
    }
  },
  created() {
    var self = this;
    self.getBorrowApplyList();
  }
};
</script>

