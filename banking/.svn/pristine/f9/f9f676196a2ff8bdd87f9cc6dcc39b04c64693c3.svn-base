    closeContract4() {
      this.dialog.contract4 = false;
      this.dialog.contract3 = true;
    },

    /**-----生成合同----- */
    async creatContract(id, apartmentId) {
      const self = this;
      self.loading.contract1 = true;
      //查询合作方数据
      const { data: basePartner } = await self.getBasePartner(apartmentId);
      if (basePartner.status.code == 200) {
        self.dialog.contract1_source_data.basePartner = basePartner.result.list;
        if (basePartner.result.list.length > 0) {
          self.dialog.contract1_source_data.basePartner_id =
            basePartner.result.list[0].id;
          self.dialog.contract1_source_data.basePartnerName =
            basePartner.result.list[0].apartmentName;
        }

        //查询生成合同数据
        const { data: creatContract } = await self.getCreatContract(id);
        if (creatContract.status.code == 200) {
          self.dialog.contract1_source_data.creatContract =
            creatContract.result;
          self.showCreatContract(creatContract.result);

          const contractId = creatContract.result.id; //合同id
          if (contractId) {
            self.dialog.contract1_source_data.contractId = contractId;
            //查询收入数据
            const { data: ratePriceList } = await self.getRatePriceList(
              contractId
            );
            if (ratePriceList.status.code == 200) {
              const { list } = ratePriceList.result;
              self.dialog.ratePriceList = list;
            }
          }
        }
      }
      self.loading.contract1 = false;
    },

    /**-----显示生成合同----- */
    showCreatContract(contract) {
      const self = this;

      //获取对应的审批数据
      let borrowApply = self.table.tbody.filter(item => {
        return item.id == self.currentItemId;
      });
      if (borrowApply.length > 0) {
        borrowApply = borrowApply[0];
      }

      self.resetCreatContract();
      //设置生成合同1弹出框的内容
      self.dialog.contract1_msg.apartmentName += "　" + contract.apartmentName;
      self.dialog.contract1_msg.apartmentAddress +=
        "　" + contract.apartmentAddress;
      self.dialog.contract1_msg.apartmentPerson +=
        "　" + contract.apartmentPerson;
      self.dialog.contract1_msg.apartmentPhone += "　" + contract.apartmentPhone;
      self.dialog.contract1_msg.borrowType +=
        "　" + (contract.borrowType == 1 ? "寓租贷" : "寓缮贷");
      self.dialog.contract1_msg.template += "　单项借款协议";
      self.dialog.contract1_msg.financeName += "　" + contract.financeName;
      self.dialog.contract1_msg.lender += "　" + contract.financePerson;
      self.dialog.contract1_msg.financePerson += "　" + contract.financePerson;
      self.dialog.contract1_msg.financeAddress += "　" + contract.financeAddress;
      self.dialog.contract1_msg.financePhone += "　" + contract.financePhone;
      self.dialog.contract1_msg.signDate += "　" + contract.signDate;
      self.dialog.contract1_msg.no += "　" + contract.no;
      self.dialog.contract1_msg.cnPrice += "　" + contract.cnPrice;
      self.dialog.contract1_msg.term +=
        "　" + contract.beginDate + "　　至　　" + contract.endDate;
      self.dialog.contract1_msg.yearLimit = "　" + contract.yearLimit + "月";
      self.dialog.contract1_msg.repayDate += "　" + contract.repayDate + "日";
      self.dialog.contract1_msg.yearRate += "　" + contract.yearRate + "%";
      self.dialog.contract1_msg.amoutMonth += "　" + contract.amoutMonth + "元";
      self.dialog.contract1_msg.payeeName += "　" + contract.payeeName;
      self.dialog.contract1_msg.bank += "　" + contract.bank;
      self.dialog.contract1_msg.bankAccount += "　" + contract.bankAccount;

      //设置生成合同2弹出框的内容
      self.dialog.contract2_msg.applicantCompany +=
        "　" + contract.apartmentName;
      self.dialog.contract2_msg.applicationType +=
        "　" + (contract.borrowType == 1 ? "寓租贷" : "寓缮贷");
      self.dialog.contract2_msg.quota += "　" + contract.price;
      self.dialog.contract2_msg.paymenTime += "　" + borrowApply.expectDate;
      self.dialog.contract2_msg.purpose += "　" + borrowApply.remark; // contract.moneyPurpose;
      self.dialog.contract2_msg.houseAddress = contract.apartmentAddress;
      self.dialog.contract2_msg.quota2 = contract.price;
      self.dialog.contract2_msg.name = contract.payeeName;
      self.dialog.contract2_msg.phone = contract.payeePhone;
      self.dialog.contract2_msg.bankName = contract.bank;
      self.dialog.contract2_msg.bankAccount = contract.bankAccount;

      //设置生成合同3弹出框的内容
      self.dialog.contract3_msg.applicantCompany +=
        "　" + contract.apartmentName;
      self.dialog.contract3_msg.houseAddress += "　" + contract.houseAddress;
      self.dialog.contract3_msg.houseNumber = "　" + contract.houseNumber + "套";
      self.dialog.contract3_msg.moneyPurpose += "　" + contract.moneyPurpose;
      self.dialog.contract3_msg.quota += "　" + contract.price;
      self.dialog.contract3_msg.yearRate += "　" + contract.yearRate + "%";
      self.dialog.contract3_msg.term +=
        "　" + contract.beginDate + "　　至　　" + contract.endDate;

      //第四弹窗内容
      self.dialog.contract4_msg.applicantCompany1 += "　" + contract.financeName;
      self.dialog.contract4_msg.applicantCompany2 +=
        "　" + contract.apartmentName;
      self.dialog.contract4_msg.beginDate += "　" + borrowApply.expectDate;
      self.dialog.contract4_msg.price += "　" + contract.price + "元";
      self.dialog.contract4_msg.term +=
        "　" + contract.beginDate + "　　至　　" + contract.endDate;
      self.dialog.contract4_msg.yearLimit = "　" + contract.yearLimit + "月";
      self.dialog.contract4_msg.moneyPurpose += "　" + contract.moneyPurpose;
      self.dialog.contract4_msg.paymentType += "　网银转账";
    },

    /**-----获取生成合同的数据----- */
    getCreatContract(id, gcid) {
      const self = this;
      const userid = tool.getUserId();
      if (!userid) {
        self.$message.error("登录超时，请先登录！");
        return;
      }
      return this.axios({
        method: "post",
        url: this.$store.state.api.borrowApply.creatContract,
        data: {
          gcid: "0100099",
          userid: userid,
          params: {
            id: id,
            gcid: gcid
          }
        }
      });
    },

    /**-----获取合作方列表----- */
    getBasePartner(apartmentId) {
      return this.axios({
        method: "post",
        url: this.$store.state.api.borrowApply.getBasepartnerList,
        data: {
          params: {
            apartmentId: apartmentId
          }
        }
      });
    },

    /**-----查询收支信息----- */
    getRatePriceList(contractId) {
      const self = this;
      const userid = tool.getUserId();
      if (!userid) {
        self.$message.error("登录超时，请先登录！");
        return;
      }
      return this.axios({
        method: "post",
        url: this.$store.state.api.borrowApply.getRatePriceList,
        data: {
          gcid: "0100099",
          userid: userid,
          params: {
            contractId: contractId
          }
        }
      });
    },

    /**-----查询预览合同的PDF----- */
    getCreatPDF(contractId) {
      return this.axios({
        method: "post",
        url: this.$store.state.api.picture.getFile,
        data: {
          params: {
            bizId: contractId,
            bigType: 6,
            smallType: 1
          }
        }
      });
    },

    /**-----更改甲方时触发----- */
    async changeBasePartner() {
      const self = this;
      self.loading.contract1 = true;
      const id = self.dialog.contract1_source_data.creatContract.applyId;
      //const gcid = self.dialog.contract1_source_data.basePartner_gcid;
      const { data: creatContract } = await self.getCreatContract(id, gcid);
      if (creatContract.status.code == 200) {
        self.dialog.contract1_source_data.creatContract =
          creatContract.result.list;
        self.showCreatContract(creatContract.result.list);
      }
      self.loading.contract1 = false;
    },

    /**-----查询列表数据----- */
    getBorrowApplyList() {
      const self = this;
      //开启loading
      self.loading.all = true;
      self
        .axios({
          method: "post",
          url: this.$store.state.api.borrowApply.getList,
          data: {
            minPrice: self.formInline.minPrice,
            maxPrice: self.formInline.maxPrice,
            pageNo: self.currentPage,
            likeName: self.formInline.likeName,
            pageSize: self.pageSize,
            startDate: self.formInline.date[0],
            endDate: self.formInline.date[1],
            status: 1 //0待审批 1已通过 2已驳回 3生成合同
          }
        })
        .then(response => {
          //关闭loading
          self.loading.all = false;
          const { data } = response;
          if (data.status.code == 200) {
            self.table.tbody = data.result.list;
            self.totalRecord = data.result.totalRecord;
          }
        })
        .catch(error => {
          self.loading.all = false;
        });
    },

    /**-----当前页改变时调用----- */
    currentChange(currentPage) {
      const self = this;
      self.currentPage = currentPage;
      self.getBorrowApplyList();
    },

    /**-----每页条目改变时调用----- */
    sizeChange(pageSize) {
      const self = this;
      self.currentPage = 1;
      self.pageSize = pageSize;
      self.getBorrowApplyList();
    },

