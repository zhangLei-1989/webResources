    /**-----显示，生成合同第1步的弹出框----- */
    openContract1(id) {
      this.dialog.contract1 = true;
      this.creatContract(id);
      this.currentItemId = id;
    },
    /**-----隐藏，生成合同第1步的弹出框----- */
    closeContract1() {
      this.dialog.contract1 = false;
    },

    /**-----显示，生成合同第2步的弹出框----- */
    openContract2() {
      this.closeContract1();
      this.dialog.contract2 = true;
    },
    /**-----隐藏，生成合同第2步的弹出框----- */
    closeContract2() {
      this.dialog.contract2 = false;
      this.dialog.contract1 = true;
    },

    /**-----显示，生成合同第3步的弹出框----- */
    openContract3() {
      this.closeContract2();
      this.dialog.contract3 = true;
      this.dialog.contract4= false;
      this.getRatePriceList();
    },
    /**-----隐藏，生成合同第3步的弹出框----- */
    closeContract3() {
      this.dialog.contract3 = false;
      this.dialog.contract2 = true;
      this.dialog.contract4 = false;
    },

    /**-----显示，生成合同第4步的弹出框----- */
    openContract4(id) {
      this.dialog.contract4 = true;
    },
    /**-----隐藏，生成合同第4步的弹出框----- */
    closeContract4() {
      this.dialog.contract4 = false;
    },

    /**-----生成合同----- */
    async creatContract(id) {
      const self = this;
      self.loading.contract1 = true;
      //查询合作方数据
      const { data: basePartner } = await self.getBasePartner();
      if (basePartner.status.code == 200) {
        const gcid =
          basePartner.result.list.length > 0
            ? basePartner.result.list[0].gcid
            : "";
        self.dialog.contract1_source_data.basePartner = basePartner.result.list;
        self.dialog.contract1_source_data.basePartner_gcid = gcid;
        if (gcid) {
          //查询生产合同数据
          const { data: creatContract } = await self.getCreatContract(id, gcid);
          if (creatContract.status.code == 200) {
            self.dialog.contract1_source_data.creatContract =
              creatContract.result.list;
            self.showCreatContract(creatContract.result.list);

            //查询收入数据
            const { data: ratePriceList } = await self.getRatePriceList(
              id,
              gcid
            );
            if (ratePriceList.status.code == 200) {
              const { list } = ratePriceList.result;
              list.forEach(item => {
                const date = new Date(item.splitStartDate);
                item.splitStartDate =
                  date.getFullYear() +
                  (date.getMonth() + 1 >= 10
                    ? date.getMonth() + 1
                    : "0" + (date.getMonth() + 1)) +
                  (date.getDay() + 1 >= 10
                    ? date.getDay() + 1
                    : "0" + (date.getDay() + 1));
                self.dialog.ratePriceList.push(item);
              });
            }
          }
        }
      }
      self.loading.contract1 = false;
    },

    /**-----显示生成合同----- */
    showCreatContract(contract) {
      const self = this;
      self.resetCreatContract();

      //设置生成合同1弹出框的内容
      self.dialog.contract1_msg.apartmentName += "　" + contract.apartmentName;
      self.dialog.contract1_msg.apartmentAddress +=
        "　" + contract.apartmentAddress;
      self.dialog.contract1_msg.apartmentPerson +=
        "　" + contract.apartmentPerson;
      self.dialog.contract1_msg.apartmentPhone += "　" + contract.apartmentPhone;
      self.dialog.contract1_msg.borrowType += "　" + contract.borrowType;
      self.dialog.contract1_msg.template += "　" + contract.template;
      self.dialog.contract1_msg.financeName += "　" + contract.financeName;
      self.dialog.contract1_msg.lender += "　" + contract.financePerson;
      self.dialog.contract1_msg.financePerson += "　" + contract.financePerson;
      self.dialog.contract1_msg.financeAddress += "　" + contract.financeAddress;
      self.dialog.contract1_msg.financePhone += "　" + contract.financePhone;
      self.dialog.contract1_msg.signDate += "　" + contract.signDate;
      self.dialog.contract1_msg.no += "　" + contract.no;
      self.dialog.contract1_msg.cnPrice += "　" + contract.cnPrice;
      self.dialog.contract1_msg.term +=
        "　" + contract.beginDate + "　　至　　" + contract.endDate;
      self.dialog.contract1_msg.yearLimit = "　" + contract.yearLimit + "年";
      self.dialog.contract1_msg.repayDate += "　" + contract.repayDate + "日";
      self.dialog.contract1_msg.yearRate += "　" + contract.yearRate + "%";
      self.dialog.contract1_msg.amoutMonth += "　" + contract.amoutMonth;
      self.dialog.contract1_msg.payeeName += "　" + contract.payeeName;
      self.dialog.contract1_msg.bank += "　" + contract.bank;
      self.dialog.contract1_msg.bankAccount += "　" + contract.bankAccount;

      //设置生成合同2弹出框的内容
      self.dialog.contract2_msg.applicantCompany +=
        "　" + contract.apartmentName;
      self.dialog.contract2_msg.applicationType += "　" + contract.borrowType;
      self.dialog.contract2_msg.quota += "　" + contract.price;
      self.dialog.contract2_msg.paymenTime += "　" + contract.beginDate;
      self.dialog.contract2_msg.purpose += "　" + contract.moneyPurpose;
      self.dialog.contract2_msg.houseAddress = contract.apartmentAddress;
      self.dialog.contract2_msg.quota2 = contract.price;
      self.dialog.contract2_msg.name = contract.payeeName;
      self.dialog.contract2_msg.phone = contract.payeePhone;
      self.dialog.contract2_msg.bankName = contract.bank;
      self.dialog.contract2_msg.bankAccount = contract.bankAccount;

      //设置生成合同3弹出框的内容
      self.dialog.contract3_msg.applicantCompany +=
        "　" + contract.apartmentName;
      self.dialog.contract3_msg.houseAddress += "　" + contract.houseAddress;
      self.dialog.contract3_msg.houseNumber = "　" + contract.houseNumber + "套";
      self.dialog.contract3_msg.moneyPurpose += "　" + contract.moneyPurpose;
      self.dialog.contract3_msg.quota += "　" + contract.price;
      self.dialog.contract3_msg.yearRate += "　" + contract.yearRate + "%";
      self.dialog.contract3_msg.term +=
        "　" + contract.beginDate + "　　至　　" + contract.endDate;

      //第四弹窗内容
      self.dialog.contract4_msg.applicantCompany1 += " "+contract.financeName;
      self.dialog.contract4_msg.applicantCompany2 += " "+contract.apartmentName;
