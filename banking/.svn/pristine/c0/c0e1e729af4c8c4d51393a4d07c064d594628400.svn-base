    //设置生成合同4弹出框的内容

    /**-----获取生成合同的数据----- */
    getCreatContract(id, gcid) {
      return this.axios({
        method: "post",
        url: this.$store.state.api.borrowApply.creatContract,
        data: {
          id: id,
          gcid: gcid
        }
      });
    },

    /**-----获取合作方列表----- */
    getBasePartner() {
      return this.axios({
        method: "post",
        url: this.$store.state.api.basePartner.getList
      });
    },

    /**-----查询收支信息----- */
    getRatePriceList(id, gcid) {
      return this.axios({
        method: "post",
        url: this.$store.state.api.borrowApply.getRatePriceList,
        data: {
          id: id,
          gcid: gcid
        }
      });
    },

    /**-----更改甲方时触发----- */
    async changeBasePartner() {
      const self = this;
      self.loading.contract1 = true;
      const id = self.dialog.contract1_source_data.creatContract.applyId;
      const gcid = self.dialog.contract1_source_data.basePartner_gcid;
      const { data: creatContract } = await self.getCreatContract(id, gcid);
      if (creatContract.status.code == 200) {
        self.dialog.contract1_source_data.creatContract =
          creatContract.result.list;
        self.showCreatContract(creatContract.result.list);
      }
      self.loading.contract1 = false;
    },

    /**-----查询列表数据----- */
    getBorrowApplyList() {
      const self = this;
      //开启loading
      self.loading.all = true;
      self
        .axios({
          method: "post",
          url: this.$store.state.api.borrowApply.getList,
          data: {
            pageNo: self.table.currentPage,
            pageSize: self.table.pageSize,
            status: 1 //0待审批 1已通过 2已驳回 3生成合同
          }
        })
        .then(response => {
          //关闭loading
          self.loading.all = false;
          const { data } = response;
          if (data.status.code == 200) {
            self.table.tbody = [];
            //动态添加数据（渲染列表页面）
            data.result.list.forEach(item => {
              self.table.tbody.push(item);
            });
            self.table.totalRecord = data.result.totalRecord;
          }
        })
        .catch(error => {
          self.loading.all = false;
        });
    },

    /**-----当前页改变时调用----- */
    currentChange(currentPage) {
      const self = this;
      self.table.currentPage = currentPage;
      self.getBorrowApplyList();
    },

    /**-----每页条目改变时调用----- */
    sizeChange(pageSize) {
      const self = this;
      self.table.currentPage = 1;
      self.table.pageSize = pageSize;
      self.getBorrowApplyList();
    },

    /**-----重置生成合同的显示信息----- */
    resetCreatContract() {
      const self = this;
      const contract1_msg = {
        borrowType: "合同类型：", //合同类型
        template: "合同模板：", //合同模板
        apartmentName: "借款人：", //借款人
        apartmentAddress: "住所地：", //借款方住所地
        apartmentPerson: "法定代表：", //借款方法定代表
        apartmentPhone: "电话：", //借款方电话
        financeName: "名字：", //乙方名字
        lender: "贷款人：", //贷款人
        financePerson: "法定代表：", //金融方法定代表
        financeAddress: "住所地：", //金融方住所地
        financePhone: "电话：", //金融方电话
        signDate: "合同签订时间：", //合同签订时间
        no: "合同编号：", //合同编号
        cnPrice: "借款金额（大写）：", //借款金额
        term: "借款期限：", //借款期限
        yearLimit: "年", //借款年限
        repayDate: "每月还本付息日：", //固定还款日
        yearRate: "借款利率年华：", //借款利率年华
        amoutMonth: "每月等额还贷本息金额：", //每月等额还贷本息金额
        payeeName: "收款人：", //收款人
        bank: "开户行：", //开户行
        bankAccount: "银行账号：" //银行账号
      };
      self.dialog.contract1_msg = contract1_msg;
    },
    /*生成合同*/
    sureCreatContract(){
      var {id} = this.currentItemId
      var signDate = this.value1;
      var  _this =this
      this.axios({
        method: "post",
        url: this.$store.state.api.borrowApply.saveContract,
        data: {
          id,
          signDate
        }
      }).then(response => {
        const { data } = response;
        if (data.status.code == 200) {
          _this.$message({
            message: '操作成功',
            type: 'success',
            duration:1000
          });
        }
      })
    }
  },
  mounted() {
  },
  created() {
    var self = this;
    self.getBorrowApplyList();
  }
};
